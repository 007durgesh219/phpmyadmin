<?php
if (count($tables) <= 1) {
    return;
}

$backquoted_tables = array();
foreach ($tables as $table) {
    array_push($backquoted_tables, PMA_Util::backquote($table));
} ?>

<strong>
    <?php echo __('Showing tables:'); ?>
    <?php echo htmlspecialchars(implode(', ', $backquoted_tables)); ?>
    <hr />
</strong>
