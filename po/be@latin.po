msgid "Slovak"
msgstr "Słavackaja"

#: libraries/messages.inc.php:1015 libraries/mysql_charsets.lib.php:287
msgid "Slovenian"
msgstr "Słavienskaja"

#: libraries/messages.inc.php:1016 pmd_general.php:95
msgid "Small/Big All"
msgstr "Zharnuć/razharnuć adlustravańnie ŭsich tablic"

#: libraries/messages.inc.php:1017 pmd_general.php:91
msgid "Snap to grid"
